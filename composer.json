{
    "name": "spryker/twig-optimizer",
    "description": "Twig extensions to optimize Twig performance",
    "keywords": ["twig", "optimzations"],
    "type": "library",
    "license": "MIT",
    "require": {
        "twig/twig": "^1.0.0 || ^2.0.0 || ^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "Spryker\\Shared\\TwigOptimizer\\": "src/Spryker/Shared/TwigOptimizer"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "SprykerTest\\Shared\\TwigOptimizer\\" : "tests/SprykerTest/Shared/TwigOptimizer"
        }
    }
}
